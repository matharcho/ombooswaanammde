                                        and Setting.Fruit.C.Enable then
                                            l = Setting.Fruit.C.HoldTime
                                            down("C", l)
                                        elseif lp.PlayerGui.Main.Skills[tool.Name]:FindFirstChild("F")
                                        and lp.PlayerGui.Main.Skills[tool.Name].F.Cooldown.AbsoluteSize.X <= 0
										and Setting.Fruit.F.Enable then
                                            l = Setting.Fruit.F.HoldTime
                                            down("F", l)
                                        else
                                            Click()
                                        end
                                    end
                                end
                            end
                        end
                        return
                    end
                    if not getgenv().FruitTransform then
                        pcall(function() EquipWeapon("Summon Sea Beast") end)
                        for _, v in pairs(lp.Character:GetChildren()) do
                            if v:IsA("Tool") then    
                                if v.ToolTip == "Melee" then
	                                getgenv().weapon = "Melee"
			                        equip("Melee")
                                    if Setting.Melee.Enable then
